{
  "name": "expo-cached-media",
  "version": "47.1.7",
  "description": "Super fast cached media component for react-native applications powered by expo",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "react-native": "lib/module/index",
  "types": "lib/typescript/index",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --skipLibCheck --noEmit",
    "build": "bob build",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScienceArtMagic/expo-cached-media.git"
  },
  "keywords": [
    "expo",
    "react-native",
    "image",
    "video",
    "media",
    "cached"
  ],
  "author": "ScienceArtMagic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ScienceArtMagic/expo-cached-media/issues"
  },
  "homepage": "https://github.com/ScienceArtMagic/expo-cached-media#readme",
  "peerDependencies": {
    "expo-file-system": "^15.2.2",
    "react": ">=18.1.0",
    "react-native": ">=0.70.5",
    "react-native-web": ">=0.18.7"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "^18.0.28",
    "@types/react-native": "^0.70.11",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "9.2.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.8.2",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "expo-file-system": "^15.2.2",
    "husky": "^4.3.8",
    "jest": "^28.1.3",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "react": ">=18.1.0",
    "react-dom": ">=18.1.0",
    "react-native": ">=0.70.5",
    "react-native-builder-bob": "^0.20.4",
    "react-native-web": ">=0.18.12",
    "typescript": "^4.9.5"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "configFile": "./babel.config.js"
        }
      ],
      "module",
      "typescript"
    ]
  }
